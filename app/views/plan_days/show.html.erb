<div class="span3">
<p>
  <b>Plan Day:</b>
  id: <%= @plan_day.id %>
  plan id: <%= @plan_day.plan_id %>
  day: <%= @plan_day.day %>
  <br>
  spots:
  <% @plan_day.spots.each do |plan_spot| %>
    name:
    <li>
      <%= plan_spot.name %>
      <%= hidden_field_tag :latitude, plan_spot.lat %>
      <%= hidden_field_tag :longitude, plan_spot.lng %>
    </li>
  <% end %>
</p>
</div>
<div class="span9">
<div id="map_canvas"></div>
</div>
<%= link_to 'Edit', edit_plan_day_path(@plan_day) %> |
<%= link_to 'Back', plan_path( @plan_day.plan ) %>
